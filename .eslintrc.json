{
    "extends": ["airbnb-base", "plugin:prettier/recommended"],
    "env": {
      "node": true,
      "es6": true,
      "jest": true
    },
    "rules": {
      "no-underscore-dangle": ["error", { "allow": ["_methods", "_endpoint"] }],
      "class-methods-use-this": "off",
      "prettier/prettier": "error"
    }
  }
  